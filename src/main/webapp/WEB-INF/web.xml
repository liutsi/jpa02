<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    

    <servlet>
        <servlet-name>VisitorServlet</servlet-name>
        <servlet-class>com.example.TestJPAServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisitorServlet</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>VisitorServlet2</servlet-name>
        <servlet-class>com.example.TestJPAServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisitorServlet2</servlet-name>
        <url-pattern>/view2</url-pattern>
    </servlet-mapping>
    
    
    <login-config></login-config>
  
	
  <display-name>Archetype Created Web Application</display-name>
</web-app>
